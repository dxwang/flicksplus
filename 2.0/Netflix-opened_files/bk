 icon-rightCaret"})),React.createElement("div",{className:i(n),ref:"sliderMask"},React.createElement("div",{className:s,ref:"sliderContent",style:a,onTouchStart:this.handleTouchStart,onTouchMove:this.handleTouchMove},this.props.children)))}});module.exports=SimpleSlider;


